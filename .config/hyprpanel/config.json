            "hide_indicators": false,
            "expand": false,
            "max_size": 0,
            "pinned": []
          }
        },
        {
          "spacer": {
            "size": 16,
            "expand": false
          }
        },
        {
          "systray": {
            "icon_size": 22,
            "menu_icon_size": 22,
            "auto_hide_statuses": [
              "STATUS_UNSPECIFIED",
              "STATUS_PASSIVE",
              "STATUS_ACTIVE"
            ],
            "auto_hide_delay": "4s",
            "pinned": [
              "blueman-applet",
              "nm-applet",
              "chrome_status_icon_1"
            ],
            "modules": [
              {
                "power": {
                  "icon_size": 22,
                  "icon_symbolic": true
                }
              }
            ]
          }
        },
        {
          "notifications": {
            "icon_size": 24,
            "notification_icon_size": 48,
            "default_timeout": "7s",
            "position": "POSITION_TOP_RIGHT",
            "margin": 24,
            "persistent": []
          }
        },
        {
          "hud": {
            "notification_icon_size": 64,
            "timeout": "2s",
            "position": "POSITION_BOTTOM",
            "margin": 256
