{
  "log_level": "LOG_LEVEL_INFO",
  "log_subprocesses_to_journal": true,
  "dbus": {
    "enabled": true,
    "connect_timeout": "20s",
    "connect_interval": "1.000s",
    "notifications": {
      "enabled": true
    },
    "systray": {
      "enabled": true
    },
    "shortcuts": {
      "enabled": true
    },
